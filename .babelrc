{
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": false,
        "useESModules": true
      }
    ],
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    [
      "component",
      {
        "libraryName": "aaa",
        "libDir": "lib",
        "camel2Dash": false,
        "style": true
      }
    ],
    ["transform-vue-jsx"]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": 2
      }
    ]
  ]
}
